<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="sw-web" extends="rpg-web" namespace="/">
	
		<global-results>
			<result name="error" type="tiles">SW-Error</result>		
		</global-results>
	
		<action name="SW-Main" class="red.arpanet.sw.web.actions.MainAction">
			<result name="input" type="tiles">SW-Main</result>
			<result name="success" type="tiles">SW-Main</result>
			<exception-mapping result="error" exception="java.lang.Exception" />
		</action>
		
		<action name="SW-Profile" class="red.arpanet.t2k.web.actions.ProfileAction">
			<result name="input" type="tiles">SW-Profile</result>
			<result name="success" type="tiles">SW-Profile</result>
			<exception-mapping result="error" exception="java.lang.Exception" />
		</action>
		
	</package>

</struts>